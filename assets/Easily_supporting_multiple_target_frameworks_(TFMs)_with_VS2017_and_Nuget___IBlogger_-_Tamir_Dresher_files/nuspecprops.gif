jQuery(window).bind("load", function() {
	var getMenuDistanceFromTop = function(what){
			var offsetTop = jQuery(what).offset().top;
			var absoluteTop =  jQuery(window).scrollTop();
			return  offsetTop - absoluteTop;
		}
		//check for reloaded page
		if (jQuery(window).scrollTop() >=  getMenuDistanceFromTop('.a2a_vertical_style')) {
			jQuery('.a2a_vertical_style').addClass("fixedIcons");
		} 
		var fixIcons = function(selection){
			var selectedElement = jQuery(selection);
			var topOffset = selectedElement.offset().top;
			//default value for menu height -- prevents zero value if menu hasn't loaded yet
			var menuHeight = 110;
			var arbitraryNudge = 0;
			var authorHeight = jQuery('.entry-content .authorinfoarea').outerHeight(true);
			var authorOffset = jQuery('.entry-content .authorinfoarea').offset().top;
			var menuBottom;
			var relDiv;
			var menuDistanceFromTop;
			var setMenuTop = function(){
			var scroll = jQuery(window).scrollTop();
				menuHeight = jQuery('.a2a_kit').outerHeight();
				//determine offset of floating menu, but only once
				if(typeof menuDistanceFromTop === 'undefined'){
					menuDistanceFromTop = getMenuDistanceFromTop('.a2a_vertical_style');
					menuBottom = menuDistanceFromTop + menuHeight;				
				}
				if((scroll+menuBottom)>(authorOffset)){
					relDiv = jQuery('.a2a_vertical_style').addClass('fixedIcons');
					relDiv.css("top", authorOffset-authorHeight-arbitraryNudge+"px");
				}else{
					relDiv = jQuery('.a2a_vertical_style').removeClass('fixedIcons');
					relDiv.css("top",menuDistanceFromTop+"px")
				}
			}

		jQuery(window).scroll(function(){
			setMenuTop();
		});
	}
	if(jQuery('body.single-post').length) {
		fixIcons('.entry-content');
	}
